{
  "parser": "babel-eslint",
  "extends": ["airbnb", "react-app", "react-app/jest", "prettier", "prettier/react"],
  "plugins": ["prettier", "react", "react-hooks", "jsx-a11y"],
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["components", "./src/components/"],
          ["constants", "./src/constants/"],
          ["utils", "./src/utils/"]
        ],
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "rules": {
    "prettier/prettier": 2,
    "no-console": 0
  }
}
